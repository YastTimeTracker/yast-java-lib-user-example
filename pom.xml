<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.yast.lib.user.example</groupId>
  <artifactId>yast-lib-user-example</artifactId>
  <packaging>jar</packaging>
  <version>1.0</version>
  <name>Yast Lib User Example</name>
  <url>http://www.yast.com</url>

  <repositories>
    <repository>
        <id>yast-lib-user-mvn-repo</id>
        <url>https://raw.github.com/YastTimeTracker/yast-java-lib-user/mvn-repo/</url>
        <snapshots>
            <enabled>true</enabled>
            <updatePolicy>always</updatePolicy>
        </snapshots>
    </repository>
  </repositories>

  <dependencies>
    <dependency>
      <groupId>com.yast.lib</groupId>
      <artifactId>yast-lib-user</artifactId>
      <version>1.0.0</version>
    </dependency>
  </dependencies>

</project>
